{
    "name": "earthquakes-dashboard-api",
    "version": 2,
    "routes": [
        { "src": "/api", "dest": "/api",  "methods": ["GET"], 
            "headers": {
              "Access-Control-Allow-Headers": "*",
              "Access-Control-Allow-Methods": "*",
              "Access-Control-Allow-Origin": "*"
            }
        },
        { "src": "/mongodb", "status": 404},
        { "src": "/*", "status": 404}
    ],
    "env": {
      "MONGODB_URI": "@earthquakes-mongodb-uri"
    }
  }